<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>0.0.2</PackageVersion>
        <Title>GraphQL.MetadataIntrospection</Title>
        <Authors>Gleb Smagliy &lt;glebsmag@gmail.com&gt;</Authors>
        <Description>Allows to introspect metadata declared by directives via query</Description>
